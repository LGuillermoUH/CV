---
import { getCollection } from 'astro:content';
import ExperienceList from '../components/ExperienceList.astro';

const experienceData = await getCollection('Experience');
---

<div class="mx-8 bg-gray-900 text-white">
  <h2 class="text-xl font-bold border-b pb-2 mb-4">Experiencia</h2>
  <ExperienceList companies={experienceData} />
</div>
